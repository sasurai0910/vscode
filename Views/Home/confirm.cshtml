@{
    ViewData["Title"] = "登録確認画面";
    @addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
}

<head>
    <link rel="stylesheet" href="~/css/confirm.css" />
</head>
<div class="text-center">
    <h2>入力内容の確認</h2>
    <ul>
        @for (int i = 0; i < Model.Count; i++)
        {
            <li>項目 @(i + 1): @Model[i]</li>
        }
    </ul>
</div>